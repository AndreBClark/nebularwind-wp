<!DOCTYPE html>
<!-- [if lt IE 9]><html class="no-js no-svg ie lt-ie9 lt-ie8 lt-ie7" {{ site.language_attributes }}> <![endif] -->
<!-- [if IE 9]><html class="no-js no-svg ie ie9 lt-ie9 lt-ie8" {{ site.language_attributes }}> <![endif] -->
<!-- [if gt IE 9]><! -->
<html class="h-full no-js no-svg" {{ site.language_attributes }}>
    <!-- <![endif] -->
{% include 'html-header.twig' %}

    <body class="{{ body_class }} h-screen flex flex-col"
        data-template="base.twig">
        <header class="header">
            {% block header %}
                <div class="text-white top-bar bg-primary">
                    <div class="container py-2 mx-auto text-left mw-screen">
                        <small class="flex justify-between">
                            <a href="tel:(866) 830-3460"
                                class="hidden tooltip tooltipstered lg:inline">
                                <i class="icon-phone"></i>
                                <span>(866) 830-3460</span>
                            </a>
                            <p class="w-full font-semibold text-center uppercase lg:w-auto">
                                Free Shipping in the U.S. on all orders over $75
                            </p>
                            <p>
                                <a href="#0" class="hidden">Newsletter</a>
                            </p>
                        </small>
                    </div>
                </div>
                <div class="container flex items-center justify-between px-2 py-4 mx-auto lg:px-0">
                    <div class="block lg:hidden">
                        <button class="flex items-center px-3 py-2 border-2 rounded text-primary border-primary hover:text-white hover:border-white">
                        <svg class="w-4 h-4 fill-current" viewBox="0 0 20 20" xmlns="http://www.w3.org/2000/svg"><title>Menu</title><path d="M0 3h20v2H0V3zm0 6h20v2H0V9zm0 6h20v2H0v-2z"/></svg>
                        </button>
                    </div>
                    <div class="font-bold uppercase hdr-logo font-headingtwo"
                        role="banner">
                        <a class="hdr-logo-link"
                            href="{{ site.url }}"
                            rel="home">
                            <img src="/wp-content/uploads/2020/03/logo-threpsitogo.png" class="h-8 lg:h-12"/>
                            <span class="sr-only">{{ site.name }}</span>
                        </a>
                    </div>
                    <nav id="nav-main"
                        class="hidden nav-main lg:flex"
                        role="navigation">
                        {% include 'menu.twig' with {
                            items: menu.get_items
                        } %}
                    </nav>
                    <!-- #nav -->
                    <div>
                        <a href="#0" class="p-2 mt-4 font-bold leading-none text-white border-2 border-solid rounded lg:px-4 lg:py-4 bg-primary border-primary hover:text-primary hover:bg-white lg:my-0">
                            Buy Now
                        </a>
                    </div>
                </div>
                    <div class="flex-grow block w-full lg:flex lg:items-center lg:w-auto">
                        <div class="text-sm lg:flex-grow">
                            <a href="#responsive-header"
                                class="block mt-4 mr-4 text-teal-200 lg:inline-block lg:mt-0 hover:text-white">
                                Docs
                            </a>
                            <a href="#responsive-header"
                                class="block mt-4 mr-4 text-teal-200 lg:inline-block lg:mt-0 hover:text-white">
                                Examples
                            </a>
                            <a href="#responsive-header"
                                class="block mt-4 text-teal-200 lg:inline-block lg:mt-0 hover:text-white">
                                Blog
                            </a>
                        </div>
                        <div>
                            <a href="#"
                                class="inline-block px-4 py-2 mt-4 text-sm leading-none text-white border border-white rounded hover:border-transparent hover:text-teal-500 hover:bg-white lg:mt-0">Download</a>
                        </div>
                    </div>
            {% endblock %}
        </header>

        <main id="content" class="container px-4 content-wrapper">
            {% if title %}
                <h1>
                    {{ title }}
                </h1>
            {% endif %}
            <div class="wrapper {{ sidebar_class }}">
                {% block content %}
                    Sorry, no content
                {% endblock %}
            </div>
            {% if sidebar %}
                <aside class="layout-sidebar">
                    {{ sidebar }}
                </aside>
            {% endif %}
        </main>

        {% block footer %}
            <footer id="footer" class="block mt-auto bg-pink-500">
                {% include 'footer.twig' %}
            </footer>
            {{ function('wp_footer') }}
        {% endblock %}
    </body>
</html>